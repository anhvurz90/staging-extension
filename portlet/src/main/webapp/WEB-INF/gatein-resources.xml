<?xml version="1.0" encoding="ISO-8859-1" ?>
<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_resources_1_3 http://www.gatein.org/xml/ns/gatein_resources_1_3"
	xmlns="http://www.gatein.org/xml/ns/gatein_resources_1_3">
	<portlet-skin>
		<application-name>staging-extension-portlet</application-name>
		<portlet-name>StagingExtensionPortlet</portlet-name>
		<skin-name>Default</skin-name>
		<css-path>/skin/css/stagingextension.css</css-path>
		<css-priority>1</css-priority>
	</portlet-skin>
	<portlet>
		<name>StagingExtensionPortlet</name>
		<module>
			<depends>
				<module>jQueryStagingExtension</module>
			</depends>
		</module>
	</portlet>
	<module>
		<name>jQueryStagingExtension</name>
		<load-group>stagingextension</load-group>
		<script>
			<path>/js/stagingextension/stagingextension.js</path>
		</script>
		<depends>
			<module>juzu-ajax</module>
		</depends>
		<depends>
			<module>jquery</module>
		</depends>
	</module>
</gatein-resources>
